{
	"name": "Reloader",
	"description": "Reload assistant",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": ["storage", "activeTab", "scripting", "tabs"],
	"action": {
		"default_popup": "./popup/popup.html"
	},
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": ["https://www.google.com/"],
			"run_at": "document_end",
			"js": ["mqtt.js", "content.js"]
		}
	]
}